{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "./",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@/*": ["./*"],
      "@resources/*": ["resources/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "types": ["node"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "../app/types/**/*.ts",
    ".next/types/**/*.ts",
    "next-env.d.ts",
    "types/**/*.ts",
    "./out/types/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
